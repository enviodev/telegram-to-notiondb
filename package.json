  "author": "",
  "license": "MIT",
  "dependencies": {
    "telegram": "^2.19.10",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "@notionhq/client": "^2.2.14"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0"
  }
}
