{
  "name": "telegram-scraper",
  "version": "1.0.0",
  "description": "CLI tool to scrape Telegram chat information from folders",
  "main": "dist/index.js",
  "scripts": {
    "update-notion": "npm run dev -- update-notion",
    "delete-duplicates": "npm run dev -- delete-duplicates",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch"
